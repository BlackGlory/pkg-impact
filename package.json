    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@blackglory/errors": "^0.1.4",
    "@blackglory/types": "^0.2.9",
    "commander": "^7.0.0",
    "cytoscape": "^3.18.0",
    "cytoscape-dagre": "^2.3.2",
    "cytoscape-klay": "^3.1.4",
    "extra-filesystem": "^0.2.3",
