{
  "name": "pkg-impact",
  "version": "0.1.12",
  "description": "Know which local packages need to be upgraded",
  "keywords": [],
  "bin": {
    "pkg-impact": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/sse-cat.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "dev": "ts-node src/cli.ts",
    "dev:debug": "node --inspect-brk node_modules/.bin/ts-node src/cli.ts",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.json",
    "build:patch": "tscpaths -p tsconfig.json -s ./src -o ./lib",
    "start": "serve dist",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "ts-node": "^9.1.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@blackglory/errors": "^1.1.2",
    "@blackglory/structures": "^0.2.1",
    "@blackglory/types": "^0.4.2",
    "commander": "^7.2.0",
    "extra-filesystem": "^0.3.2",
    "extra-promise": "^0.17.1",
    "glob": "^7.1.7",
    "iterable-operator": "^0.14.0",
    "return-style": "^0.12.3",
    "ts-is-present": "^1.2.1"
  }
}
