{
  "name": "pkgraph",
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "bin": {
    "pkgraph": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/sse-cat.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "dev:web": "webpack serve --config webpack.dev.js",
    "dev:cli": "ts-node src/index.ts",
    "dev:cli:debug": "node --inspect-brk node_modules/.bin/ts-node src/index.ts",
    "dev:build": "run-p dev:build:*",
    "dev:build:cli": "run-s build:cli",
    "dev:build:web": "webpack --config webpack.dev.js",
    "prepublishOnly": "run-s clean build",
    "clean": "run-p clean:*",
    "clean:web": "rimraf dist",
    "clean:cli": "rimraf lib",
    "build": "run-p build:*",
    "build:cli": "run-s build:cli:*",
    "build:cli:compile": "tsc --project tsconfig.json",
    "build:cli:patch": "tscpaths -p tsconfig.json -s ./src -o ./lib",
    "build:web": "webpack --config webpack.prod.js",
    "start": "serve dist",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
